<?php 
//     *
//    ***
//   *****
//  *******
// *********
$z=1;
for($i=1;$i<=5;$i++)
{
	for($j=4;$j>=$i;$j--)
	{
		echo "&nbsp ";
	}
	for($k=1;$k<=$z;$k++)
	{
		echo "*";
	}
	$z+=2;
	echo "<br>";
}
?>


