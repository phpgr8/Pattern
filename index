<?php 
$ary=[12,13,12,30,32,44,30];
for($i=1;$i<=500;$i++)
{
	$j=$i;
	$rev=0;
	while($j>0)
	{
		$rev=$rev+($j%10)*($j%10)*($j%10);
		$j=(int)($j/10);
	}

	if($rev==$i)
	{
		echo $i.",";
	}
}

?>
<br><br>

<?php 
// * * *
// * * *
// * * *

for($i=1;$i<=3;$i++)
{
	for($j=1;$j<=3;$j++)
	{
		echo "*";
	}
	echo "<br>";
}
?>
<br><br>

